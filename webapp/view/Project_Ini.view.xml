<mvc:View controllerName="QM_Admin.controller.Project_Ini" xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml" height="100%"
	xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Page title="{i18n>projectOview}" id = "pgeProjectPage">
		<f:SimpleForm id="sfmIniProject" editable="true" layout="ResponsiveGridLayout" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0"
			columnsL="2" columnsM="2" class="editableForm">
			<f:toolbar>
				<Toolbar id="tbrProject_Ini">
					<Button id="btnGo" icon="sap-icon://arrow-right" text="Continue" />
				</Toolbar>
			</f:toolbar>
			<f:content>
				<Label id="lblProjectName" text="{i18n>projectName}"/>
				<Input id="iptProjectName">
					<layoutData>
						<l:GridData span="L2 M2 S3"/>
					</layoutData>
				</Input>
				<!--	<layoutData>-->
				<!--		<l:GridData span="L1 M1 S1"/>-->
				<!--	</layoutData>-->
				<!--</Button>-->
			</f:content>
		</f:SimpleForm>
	</Page>
</mvc:View>